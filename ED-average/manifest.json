{
	"name": "ED Average",
	"version": "1.0",
	"manifest_version": 2,
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": ["content.js"],
		"css": ["style.css"]
	}],
	"browser_action": {
		"default_icon": "logoEcoleDirecte.png",
		"default_title": "EcoleDirecte Average",
		"default_popup": "popup.html"
	},
	"permissions": [
		"<all_urls>",
		"storage",
		"tabs",
		"webNavigation",
		"activeTab"
	]
}
